&lt;p&gt;Lorem ipsum dolor sit &lt;/p&gt;
p {  text-indent: 10%;  }

  &lt;ul&gt;
          &lt;li&gt;li:nth-child(2n+1)&lt;/li&gt;
          &lt;li&gt;list&lt;/li&gt;
          &lt;li&gt;li:nth-child(2n+1)&lt;/li&gt;
          &lt;li&gt;list&lt;/li&gt;
          &lt;li&gt;li:nth-child(2n+1)&lt;/li&gt;
 &lt;/ul&gt;

li:nth-child(2n+1) {
  text-indent: 5px;
  font-weight: bold;
}

li:nth-child(2n) {
  text-indent: 15px;
  font-style: italic;
}</rich_text>
