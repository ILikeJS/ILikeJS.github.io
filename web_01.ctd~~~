			<rich_text foreground="#ffffa5a50000" scale="h2" weight="heavy">	 </rich_text>
			<rich_text scale="h2">                 перейти в директорию '/home'
</rich_text>
			<rich_text foreground="#ffffa5a50000" scale="h2" weight="heavy">cd ..</rich_text>
			<rich_text scale="h2">	                            перейти в директорию уровнем выше
</rich_text>
			<rich_text foreground="#ffffa5a50000" scale="h2" weight="heavy">cd ../..	</rich_text>
			<rich_text scale="h2">                        перейти в директорию двумя уровнями выше
</rich_text>
			<rich_text foreground="#ffffa5a50000" scale="h2" weight="heavy">cd ~ </rich_text>
			<rich_text scale="h2">	                        перейти в домашнюю директорию
</rich_text>
			<rich_text foreground="#ffffa5a50000" scale="h2" weight="heavy">cd -	</rich_text>
			<rich_text scale="h2">                           перейти в директорию, в которой находились до перехода в текущую директорию
</rich_text>
			<rich_text foreground="#ffffa5a50000" scale="h2" weight="heavy">mkdir </rich_text>
			<rich_text foreground="#ffffa5a50000" scale="h2" style="italic" weight="heavy">dir1</rich_text>
			<rich_text scale="h2">	              создать директорию с именем 'dir1'
</rich_text>
			<rich_text foreground="#ffffa5a50000" scale="h2" weight="heavy">echo &quot;&quot; &gt; </rich_text>
			<rich_text foreground="#ffffa5a50000" scale="h2" style="italic" weight="heavy">file.txt</rich_text>
			<rich_text scale="h2">
</rich_text>
			<rich_text foreground="#ffffa5a50000" scale="h2" weight="heavy">echo &quot;</rich_text>
			<rich_text foreground="#000000000000" scale="h2" style="italic">добавлю текст, не затирая предыдущий</rich_text>
			<rich_text foreground="#ffffa5a50000" scale="h2" weight="heavy">&quot; &gt;&gt; file.txt</rich_text>
			<rich_text scale="h2">



</rich_text>
			<rich_text scale="h2" weight="heavy">Linux   terminal: </rich_text>
			<rich_text scale="h2">   </rich_text>
			<rich_text scale="h2" style="italic">чувствительными к регистру букв</rich_text>
			<rich_text scale="h2">
</rich_text>
			<rich_text foreground="#ffffa5a50000" scale="h2" weight="heavy">pwd</rich_text>
			<rich_text scale="h2">		                       показать название и путь текущей директории
</rich_text>
			<rich_text foreground="#ffffa5a50000" scale="h2" weight="heavy">ls</rich_text>
			<rich_text scale="h2">	                             отобразить содержимое текущей директории
</rich_text>
			<rich_text foreground="#ffffa5a50000" scale="h2" weight="heavy">rm -rf </rich_text>
			<rich_text foreground="#ffffa5a50000" scale="h2" style="italic" weight="heavy">dir1</rich_text>
			<rich_text scale="h2">		          удалить директорию с именем 'dir1' и рекурсивно всё её содержимое;
</rich_text>
			<rich_text foreground="#ffffa5a50000" scale="h2" weight="heavy">rm </rich_text>
			<rich_text foreground="#ffffa5a50000" scale="h2" style="italic" weight="heavy">file.txt</rich_text>
			<rich_text scale="h2" weight="heavy"> </rich_text>
			<rich_text scale="h2">               удалить file.txt


</rich_text>
